{
    "manifest_version": 2,
    "name": "Remove Elements by Class",
    "version": "1.0",
    "description": "A Chrome extension to remove elements with a specific class name.",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}